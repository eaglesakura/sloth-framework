<?xml version="1.0" encoding="utf-8"?>
<resources>

    <dimen name="EsMaterial.Progress.MinWidth">240dp</dimen>
    <dimen name="EsMaterial.Progress.BarHeight">3dp</dimen>
    <color name="EsMaterial.Progress.Background">#7F000000</color>

    <integer-array name="EsMaterial_Widget_Progress_ColorTable">
        <item>@color/EsMaterial.LightGreen.400</item>
    </integer-array>

    <style name="EsMaterial.Theme.Progress" parent="SmoothProgressBar">
        <item name="spb_colors">@array/EsMaterial_Widget_Progress_ColorTable</item>
        <item name="spb_interpolator">spb_interpolator_decelerate</item>
    </style>
    <declare-styleable name="MaterialProgressView">
        <attr name="esmText" format="string"/>
    </declare-styleable>

</resources>